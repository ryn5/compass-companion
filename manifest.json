{
    "manifest_version": 2,
    "name": "compass-companion",
    "description": "A chrome extension for your Compass card",
    "version": "1.0",
    "icons": {
        "128": "images/orange.jpeg"
    },
    "browser_action": {
        "default_icon": "images/blue.jpeg",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]
}